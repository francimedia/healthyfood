<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>{% block title %}{% endblock %} Dashboard</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- Le styles -->
    <link href="/assets/manager/css/bootstrap.css" rel="stylesheet">
    <style type="text/css">
      body {
        padding-top: 60px;
        padding-bottom: 40px;
      }
    </style>
    <link href="/assets/manager/css/bootstrap-switch.css" rel="stylesheet">
    <link href="/assets/manager/css/bootstrap-responsive.css" rel="stylesheet">


    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="/assets/manager/js/html5shiv.js"></script>
    <![endif]-->
 
  </head>

  <body>

    <div class="navbar navbar-inverse navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="brand" href="#">Healthy Food Compass - Analytics</a>
          <div class="nav-collapse collapse">
            <ul class="nav">
              <li class="active"><a href="/app/stats">Home</a></li>
            </ul> 
          </div><!--/.nav-collapse -->
        </div>
      </div>
    </div>

    <div class="container">

      {% block content %}{% endblock %}

      <hr>

      {% block footer %}
      <footer>
        <p>&copy; 2013</p>
      </footer>
      {% endblock %}

    </div> <!-- /container -->

    {% block scripts %}
    <script src="/assets/manager/js/jquery.js"></script>
    <script src="/assets/manager/js/bootstrap.min.js"></script> 
    <script src="/assets/manager/js/jquery.switch.js"></script> 
    <script src="/assets/manager/js/bootstrap-multiselect.js"></script> 
    
    {% endblock %}

  </body>
</html> 